generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post{
  id String @id @default(uuid())
  title String
  description String
  imageUrl String
  authorName String
  authorId String
  authorImage String
  createdAt DateTime @default(now())
}
